{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "chat-ollama": "Endpoint=http://localhost:11434;Model=llama3.2",
    "embed-ollama": "Endpoint=http://localhost:11434;Model=nomic-embed-text"
  },
  "AppSettings": {
      "EmbeddingBatchSize": 32,
      "MaxTokensPerLine": 300,
      "MaxTokensPerParagraph": 1000,
      "OverlapTokens": 100,
      "MaxRelevantChunks": 50,
      "MaxInputTokens": 32768,
      "MaxOutputTokens": 800,
      "MessageExpiration": "00:05:00",
      "MessageLimit": 20
  }
}
